generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  password String
  name     String?
}

model Course {
  id          Int     @id @default(autoincrement())
  image       String
  title       String  @unique
  description String
  content     String?
}